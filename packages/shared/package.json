{
  "name": "@uploadthing/shared",
  "version": "6.5.0",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint src test --max-warnings 0",
    "build": "bunchee --tsconfig tsconfig.build.json",
    "clean": "git clean -xdf dist node_modules",
    "dev": "wait-on ../mime-types/dist/index.d.ts && bunchee -w --tsconfig tsconfig.build.json --no-clean",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@uploadthing/eslint-config": "workspace:0.2.0",
    "@uploadthing/mime-types": "workspace:0.2.7",
    "@uploadthing/tsconfig": "workspace:0.1.0",
    "bunchee": "^5.1.2",
    "eslint": "^8.57.0",
    "typescript": "^5.4.2",
    "wait-on": "^7.2.0"
  },
  "peerDependencies": {
    "@uploadthing/mime-types": "^0.2.2"
  },
  "peerDependenciesMeta": {
    "@uploadthing/mime-types": {
      "optional": true
    }
  },
  "eslintConfig": {
    "root": true,
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            "@uploadthing/shared",
            "@uploadthing/shared/*"
          ]
        }
      ]
    },
    "extends": [
      "@uploadthing/eslint-config/base"
    ]
  },
  "dependencies": {
    "std-env": "^3.7.0"
  }
}
