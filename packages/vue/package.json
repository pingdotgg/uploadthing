{
  "name": "@uploadthing/vue",
  "version": "5.1.0",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./style.css": "./dist/style.css",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
