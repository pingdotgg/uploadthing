  "dependencies": {
    "@uploadthing/shared": "^5.0.1",
    "@vueuse/core": "^10.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.22",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "uploadthing": "5.1.0",
    "vite": "^4.3.9",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.2"
  },
  "peerDependencies": {
    "uploadthing": "5.1.0",
    "vue": "^3.2.47"
  }
