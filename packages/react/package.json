{
  "name": "@uploadthing/react",
  "version": "7.3.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github:pingdotgg/uploadthing",
    "directory": "packages/react"
  },
  "sideEffects": false,
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./styles.css": "./dist/index.css",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./native": {
      "import": {
        "types": "./native/index.d.ts",
        "default": "./native/index.js"
      },
      "require": {
        "types": "./native/index.d.cts",
        "default": "./native/index.cjs"
      }
    },
    "./next-ssr-plugin": {
      "import": {
        "types": "./next-ssr-plugin/index.d.ts",
        "default": "./next-ssr-plugin/index.js"
      },
      "require": {
        "types": "./next-ssr-plugin/index.d.cts",
        "default": "./next-ssr-plugin/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "native",
    "next-ssr-plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --no-clean",
    "clean": "git clean -xdf dist hooks native next-ssr-plugin node_modules",
    "lint": "eslint src test --max-warnings 0",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uploadthing/shared": "workspace:*",
    "file-selector": "0.6.0"
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.2 || ^18.0.0 || ^19.0.0",
    "uploadthing": "^7.2.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@uploadthing/eslint-config": "workspace:*",
    "@uploadthing/tsconfig": "workspace:*",
    "@uploadthing/vitest-config": "workspace:*",
    "@vitest/browser": "4.0.15",
    "@vitest/coverage-istanbul": "4.0.15",
    "concurrently": "^9.1.2",
    "eslint": "9.39.1",
    "msw": "2.12.4",
    "next": "16.0.9",
    "react": "19.2.2",
    "react-dom": "19.2.2",
    "tailwindcss": "^4.1.18",
    "tsdown": "0.17.2",
    "typescript": "5.9.3",
    "uploadthing": "workspace:*",
    "vitest": "4.0.15",
    "vitest-browser-react": "2.0.2",
    "wait-on": "^9.0.3",
    "zod": "^3.24.1"
  }
}
