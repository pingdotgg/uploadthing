{
  "name": "@uploadthing/expo",
  "version": "7.2.6",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github:pingdotgg/uploadthing",
    "directory": "packages/expo"
  },
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint src --max-warnings 0",
    "build": "tsdown",
    "clean": "git clean -xdf dist node_modules",
    "dev": "tsdown --no-clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uploadthing/react": "workspace:*",
    "@uploadthing/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "19.2.7",
    "@uploadthing/eslint-config": "workspace:*",
    "@uploadthing/tsconfig": "workspace:*",
    "eslint": "9.39.2",
    "expo-constants": "^18.0.12",
    "expo-document-picker": "^14.0.8",
    "expo-image-picker": "^17.0.10",
    "react": "19.1.4",
    "react-native": "~0.81.5",
    "tsdown": "0.17.4",
    "typescript": "5.9.3",
    "uploadthing": "workspace:*",
    "wait-on": "^9.0.3"
  },
  "peerDependencies": {
    "expo-constants": "*",
    "expo-document-picker": "*",
    "expo-image-picker": "*",
    "react": "*",
    "react-native": "*",
    "uploadthing": "^7.2.0"
  },
  "peerDependenciesMeta": {
    "@uploadthing/mime-types": {
      "optional": true
    }
  }
}
