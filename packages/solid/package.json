{
  "name": "@uploadthing/solid",
  "type": "module",
  "author": "OrJDev",
  "license": "MIT",
  "version": "6.1.3",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "exports": {
    ".": {
      "worker": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": "./dist/server.cjs"
      },
      "browser": {
        "solid": {
          "development": "./dist/dev.jsx",
          "import": "./dist/index.jsx"
        },
        "development": {
          "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/dev.js"
          },
          "require": "./dist/dev.cjs"
        },
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": "./dist/index.cjs"
      },
      "deno": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": "./dist/server.cjs"
      },
      "node": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": "./dist/server.cjs"
      },
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        },
        "require": "./dist/dev.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && tsup --config ./tsup.config.js",
    "dev": "wait-on ../uploadthing/server/index.d.ts && tsup --watch --config ./tsup.config.js",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@effect/schema": "^0.62.6",
    "@uploadthing/dropzone": "^0.1.3",
    "@uploadthing/shared": "^6.3.1",
    "effect": "^2.3.5",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer": "10.4.17",
    "postcss": "8.4.35",
    "solid-js": "^1.8.15",
    "tailwindcss": "^3.4.1",
    "tsup": "8.0.2",
    "tsup-preset-solid": "0.1.8",
    "typescript": "^5.3.3",
    "uploadthing": "6.4.1",
    "wait-on": "^7.2.0"
  },
  "peerDependencies": {
    "solid-js": "^1.5.3",
    "uploadthing": "^6.5.0"
  },
  "keywords": [
    "SolidJS",
    "Uploadthing"
  ],
  "eslintConfig": {
    "root": true,
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            "@uploadthing/solid",
            "@uploadthing/solid/*"
          ]
        }
      ]
    },
    "extends": [
      "@uploadthing/eslint-config/base"
    ]
  }
}
