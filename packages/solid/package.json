{
  "name": "@uploadthing/solid",
  "type": "module",
  "author": "OrJDev",
  "license": "MIT",
  "version": "6.4.4",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "exports": {
    ".": {
      "worker": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "browser": {
        "solid": {
          "development": "./dist/dev.jsx",
          "import": "./dist/index.jsx"
        },
        "development": {
          "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/dev.js"
          },
          "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/dev.cjs"
          }
        },
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "deno": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "node": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/dev.cjs"
        }
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf dist node_modules",
    "dev": "wait-on ../uploadthing/server/index.d.ts && tsup --watch",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uploadthing/dropzone": "workspace:*",
    "@uploadthing/shared": "workspace:*",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "postcss": "8.4.38",
    "solid-js": "^1.8.15",
    "tailwindcss": "^3.4.1",
    "tsup": "8.0.2",
    "tsup-preset-solid": "2.2.0",
    "typescript": "^5.4.5",
    "uploadthing": "workspace:*",
    "wait-on": "^7.2.0"
  },
  "peerDependencies": {
    "solid-js": "^1.5.3",
    "uploadthing": "^6.10.0"
  },
  "keywords": [
    "SolidJS",
    "Uploadthing"
  ],
  "eslintConfig": {
    "root": true,
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            {
              "group": [
                "@uploadthing/solid",
                "@uploadthing/solid/*"
              ],
              "message": "Use relative src imports instead"
            }
          ],
          "paths": [
            {
              "name": "effect",
              "message": "Use alias imports instead (import * as X from \"effect/X\")"
            }
          ]
        }
      ]
    },
    "extends": [
      "@uploadthing/eslint-config/base"
    ]
  }
}
