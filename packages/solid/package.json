{
  "name": "@uploadthing/solid",
  "type": "module",
  "author": "OrJDev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:pingdotgg/uploadthing",
    "directory": "packages/solid"
  },
  "version": "7.3.2",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "exports": {
    ".": {
      "worker": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "browser": {
        "solid": {
          "development": "./dist/dev.jsx",
          "import": "./dist/index.jsx"
        },
        "development": {
          "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/dev.js"
          },
          "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/dev.cjs"
          }
        },
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "deno": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "node": {
        "solid": "./dist/server.jsx",
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/server.cjs"
        }
      },
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/dev.cjs"
        }
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf dist node_modules",
    "dev": "wait-on ../uploadthing/server/index.d.ts && tsup --watch",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uploadthing/shared": "workspace:*",
    "file-selector": "0.6.0"
  },
  "devDependencies": {
    "@uploadthing/eslint-config": "workspace:*",
    "eslint": "9.25.1",
    "postcss": "8.4.49",
    "solid-js": "^1.9.3",
    "tailwindcss": "^3.4.16",
    "tsup": "8.3.5",
    "tsup-preset-solid": "2.2.0",
    "typescript": "5.8.3",
    "uploadthing": "workspace:*",
    "wait-on": "^8.0.1"
  },
  "peerDependencies": {
    "solid-js": "^1.5.3",
    "uploadthing": "^7.2.0"
  },
  "keywords": [
    "SolidJS",
    "Uploadthing"
  ]
}
