{
  "name": "@uploadthing/mime-types",
  "version": "0.3.6",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "github:pingdotgg/uploadthing",
    "directory": "packages/mime-types"
  },
  "sideEffects": false,
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./application": {
      "import": {
        "types": "./application/index.d.mts",
        "default": "./application/index.mjs"
      },
      "require": {
        "types": "./application/index.d.cts",
        "default": "./application/index.cjs"
      }
    },
    "./audio": {
      "import": {
        "types": "./audio/index.d.mts",
        "default": "./audio/index.mjs"
      },
      "require": {
        "types": "./audio/index.d.cts",
        "default": "./audio/index.cjs"
      }
    },
    "./image": {
      "import": {
        "types": "./image/index.d.mts",
        "default": "./image/index.mjs"
      },
      "require": {
        "types": "./image/index.d.cts",
        "default": "./image/index.cjs"
      }
    },
    "./text": {
      "import": {
        "types": "./text/index.d.mts",
        "default": "./text/index.mjs"
      },
      "require": {
        "types": "./text/index.d.cts",
        "default": "./text/index.cjs"
      }
    },
    "./video": {
      "import": {
        "types": "./video/index.d.mts",
        "default": "./video/index.mjs"
      },
      "require": {
        "types": "./video/index.d.cts",
        "default": "./video/index.cjs"
      }
    }
  },
  "files": [
    "application",
    "audio",
    "dist",
    "image",
    "text",
    "video"
  ],
  "scripts": {
    "lint": "eslint src --max-warnings 0",
    "build": "tsdown",
    "clean": "git clean -xdf dist node_modules",
    "dev": "tsdown --no-clean",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@uploadthing/eslint-config": "workspace:*",
    "@uploadthing/tsconfig": "workspace:*",
    "eslint": "9.39.2",
    "tsdown": "0.17.4",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
