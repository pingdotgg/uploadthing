    "@types/react": "19.2.7",
    "@uploadthing/eslint-config": "workspace:*",
    "@uploadthing/tsconfig": "workspace:*",
    "@uploadthing/vitest-config": "workspace:*",
    "@vitest/coverage-istanbul": "4.0.15",
    "body-parser": "^2.2.1",
    "eslint": "9.25.1",
    "express": "^5.0.1",
    "fastify": "^5.2.0",
    "h3": "^1.13.0",
    "msw": "2.12.4",
    "next": "16.0.10",
    "react-router": "^7.10.1",
    "solid-js": "^1.9.3",
    "tailwindcss": "^4.1.18",
    "tsdown": "0.17.3",
    "typescript": "5.9.3",
    "valibot": "1.2.0",
    "vitest": "4.0.15",
    "vue": "^3.4.21",
    "wait-on": "^9.0.3",
    "zod": "^3.24.1"
  }
