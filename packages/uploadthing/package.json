{
  "name": "uploadthing",
  "version": "7.7.4",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18.13.0"
  },
  "repository": {
    "type": "git",
    "url": "github:pingdotgg/uploadthing",
    "directory": "packages/uploadthing"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
