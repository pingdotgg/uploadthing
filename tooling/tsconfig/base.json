{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "./base.build.json",
  "compilerOptions": {
    "baseUrl": "../..",
    "paths": {
      "@uploadthing/dropzone/*": ["packages/dropzone/src/*"],
      "@uploadthing/shared": ["packages/shared/src"],
      "@uploadthing/mime-types": ["packages/mime-types/src"],
      "@uploadthing/react": ["packages/react/src"],
      "@uploadthing/react/*": ["packages/react/src/*"],
      "@uploadthing/expo": ["packages/expo/src"],
      "@uploadthing/expo/*": ["packages/expo/src/*"],
      "@uploadthing/solid": ["packages/solid/src"],
      "uploadthing/*": ["packages/uploadthing/src/*"]
