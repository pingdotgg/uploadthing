    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "effect": "3.14.21",
    "elysia": "^1.2.9",
    "express": "^5.0.1",
    "fastify": "^5.2.0",
    "h3": "^1.13.0",
    "hono": "^4.6.15",
    "listhen": "^1.9.0",
    "uploadthing": "7.7.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241230.0",
    "@types/bun": "^1.2.5",
    "@types/cors": "^2.8.17",
