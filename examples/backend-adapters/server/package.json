    "@sinclair/typebox": "^0.34.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "effect": "3.11.5",
    "elysia": "^1.1.16",
    "express": "^4.18.2",
    "fastify": "^4.26.1",
    "h3": "^1.11.1",
    "hono": "^4.0.8",
    "listhen": "^1.7.2",
    "uploadthing": "7.4.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
