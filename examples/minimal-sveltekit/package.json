    "clean": "git clean -xdf dist .svelte-kit node_modules",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.17",
    "@uploadthing/svelte": "7.3.2",
    "uploadthing": "7.7.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.15.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "svelte": "^5.16.1",
    "svelte-check": "^4.1.1",
    "typescript": "5.8.3",
    "vite": "^6.3.4"
  },
  "type": "module"
}
